<?xml version="1.0" encoding="UTF-8"?>
<cacheConfig enableLocalCache="true"
	enableRemoteCache="false" ehcahceConfigFile="singleehcache.xml">
	<list>
		<cache name="dcom:space" keyField="t_id" targetType="bean">
			<localCache />
		</cache>
		<cache name="dcom:category" keyField="id" targetType="bean">
			<localCache />
		</cache>
		<cache name="dcom:user" keyField="id" targetType="bean">
			<localCache/>
		</cache>
		<cache name="dcom:event" keyField="eventId" targetType="bean">
			<localCache />
		</cache>
		
		<!-- 事件等级 -->
		<cache name="dcom:event:level" keyField="level" targetType="bean">
			<localCache />
		</cache>
		
		<!-- 事件数量 -->
		<cache name="dcom:event:count" keyField="" targetType="integer">
			<localCache />
		</cache>

		<cache name="dcom:auth" keyField="token" targetType="bean">
			<localCache />
		</cache>
		
		<cache name="dcom:resource" keyField="userId" targetType="bean">
			<localCache />
		</cache>
		
		<cache name="dcom:capacity" keyField="objName" targetType="bean">
			<localCache />
		</cache>

		<cache name="dcom:dutysign" keyField="" targetType="bean">
			<localCache />
		</cache>
	</list>
</cacheConfig>